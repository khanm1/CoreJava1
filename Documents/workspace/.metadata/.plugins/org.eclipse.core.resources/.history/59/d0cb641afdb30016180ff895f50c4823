package classNobject;

public class House {
	//Variables
	public int adress = 1625;
	public String className;
	
	//Method
	public void watchTVShows () {
		System.out.println("Watching collage football game");
		
	}
	public void sleep () {
		System.out.println("Watching collage football game");
		
	}
	
